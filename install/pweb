echo -e "\033[1;37mâ”£â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”›\033[0m"
echo -ne "\033[1;37mâ”—â”â”«\033[1;32mO QUE DESEJA FAZER \033[1;31m? "; read -r x

case "$x" in 
   1 | 01)
   painel_att
   exit;
   ;;
   2 | 02)
   painel_empresa
   exit;
   ;;
   3 | 03)
   painel_rest
   exit;
   ;;
   4 | 04)
   oracl
   exit;
   ;;
   5 | 05)
   att_pweb
   exit;
   ;;
   6 | 06)
   senha_root
   exit;
   ;;
    7 | 07)
   bot_telegram
   exit;
   ;;
    8 | 08)
   clear
   velocity
   echo ""
   echo -ne "\n\033[1;31mENTER \033[1;33mpara retornar ao \033[1;32mPWEB! \033[0m"; read
