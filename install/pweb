   exit;
   ;;
    9 | 09)
   autoexec
   exit;
   ;;
   10 | 010)
   echo ""
   echo -ne "\033[1;36mDESEJA REALMENTE REINICIAR [S/N]: \033[1;37m"; read x
   [[ $x = @(n|N) ]] && pweb
   time_reboot "10"
   exit;
   ;;
   11 | 011)
   painel_whatsapp
   exit;
   ;;
   0 | 00)
   echo -e "\033[1;31mSaindo...\033[0m"
   sleep 2
   clear
   exit;
   ;;
   *)
   echo -e "\n\033[1;31mOpÃ§Ã£o invÃ¡lida !\033[0m"
   sleep 2
