./verifatt.sh
cd  || exit;
verp=$(cat /bin/ppweb/verpweb) > /dev/null 2>&1
verw=$(cat /bin/ppweb/verweb) > /dev/null 2>&1
verap=$(cat /bin/ppweb/attpweb) > /dev/null 2>&1
veraw=$(cat /bin/ppweb/attweb) > /dev/null 2>&1
echo "America/Sao_Paulo" > /etc/timezone &>/dev/null
ln -fs /usr/share/zoneinfo/America/Sao_Paulo /etc/localtime &>/dev/null
dpkg-reconfigure --frontend noninteractive tzdata &>/dev/null
clear
echo -e "\033[1;37mâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”“\033[0m"
echo -e "\033[1;37mâ”ƒ                       \033[38;5;118mPAINEL \033[38;5;190mSONYC SSH\033[38;5;197m-\033[38;5;164mSS\033[38;5;203mH  \033[38;5;197m\033[1;37m                      â”ƒ\E[0m"
echo -e "\033[1;1;37mâ”ƒ   \033[1;33m[\033[1;31m!\033[1;33m]       \033[1;31mINSTALADA \033[1;32mVERSÃƒO: $verp \033[1;31mDO PWEB              \033[1;33m[\033[1;31m!\033[1;33m]\033[0m   \033[1;1;37mâ”ƒ"
[[ "$verp" = "$verap" ]]  && {
    sleep 0.2
} || {
echo -e "\033[1;1;37mâ”ƒ   \033[5;33m[\033[1;31m!\033[1;33m]     \033[1;31mATUALIZAÃ‡ÃƒO \033[1;32mVERSÃƒO: $verap \033[1;31mPARA O PWEB          \033[1;33m[\033[1;31m!\033[1;33m]\033[0m   \033[1;1;37mâ”ƒ"
}
echo -e "\033[1;1;37mâ”ƒ   \033[1;33m[\033[1;31m!\033[1;33m]       \033[1;31mINSTALADA \033[1;32mVERSÃƒO: $verw \033[1;31mDO PAINEL WEB          \033[1;33m[\033[1;31m!\033[1;33m]\033[0m  \033[1;1;37mâ”ƒ"
[[ "$verw" = "$veraw" ]]  && {
    sleep 0.2
} || {
echo -e "\033[1;1;37mâ”ƒ   \033[5;33m[\033[1;31m!\033[1;33m]     \033[1;31mATUALIZAÃ‡ÃƒO \033[1;32mVERSÃƒO: $veraw \033[1;31mPARA O PAINEL WEB     \033[1;33m[\033[1;31m!\033[1;33m]\033[0m   \033[1;1;37mâ”ƒ"
}
echo -e "\033[1;37mâ”£â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”«\033[0m"
echo -e "\033[1;37mâ”ƒ\033[38;5;82mSISTEMA              \033[1;37m| \033[38;5;82mMEMÃ“RIA RAM          \033[1;37m| \033[38;5;82mPROCESSADOR     \033[1;37mâ”ƒ"
